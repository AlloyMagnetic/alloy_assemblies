<div class="hubspot-cta">
  <span class="hs-cta-wrapper" id="hs-cta-wrapper-{{ uuid|raw }}">
  <span class="hs-cta-node hs-cta-{{ uuid|raw }}" id="hs-cta-{{ uuid|raw }}">
  <!--[if lte IE 8]><div id="hs-cta-ie-element"></div><![endif]-->
  <a href="http://cta-redirect.hubspot.com/cta/redirect/{{ hubspot_id|raw }}/{{ uuid|raw }}">
    <img class="hs-cta-img" id="hs-cta-img-{{ uuid|raw }}" style="border-width:0px;" src="https://no-cache.hubspot.com/cta/default/{{ hubspot_id|raw }}/{{ uuid|raw }}.png" />
  </a>

  <script charset="utf-8" src="https://js.hscta.net/cta/current.js"></script>
  <script type="text/javascript">hbspt.cta.load('{{ hubspot_id|raw }}', '{{uuid|raw}}');</script>
</div>
